<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.ContactPicker"
            prefHeight="200.0" prefWidth="410.0">

    <headerText>
        Adding New Contact.
    </headerText>
    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="First Name: " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="firstnamePicker" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Last Name: " GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="lastnamePicker" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Phone Number: " GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="numberPicker" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label text="Notes:" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="notePicker" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        </GridPane>
    </content>

</DialogPane>
